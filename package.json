    "vue-loader": "^16.0.0-beta.5",
    "vue-markdown-loader": "^2.4.1",
    "vue-router": "^4.0.0-beta.4",
    "vue-style-loader": "^4.1.2",
    "vuex": "^4.0.0-beta.4",
    "webpack": "^4.44.1",
